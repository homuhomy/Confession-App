<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane onInputMethodTextChanged="#switchScenes" prefHeight="480.0" prefWidth="765.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ds.confessionapp.submittedPageController">
   <children>
      <Label layoutX="209.0" layoutY="174.0" text="Your confession has been submitted!">
         <font>
            <Font name="PP Pangram Sans Extrabold Italic" size="19.0" />
         </font>
      </Label>
      <Label layoutX="167.0" layoutY="213.0" text="Hang tight! Your confession will take a while before it will be posted.">
         <font>
            <Font name="PP Pangram Sans Light" size="14.0" />
         </font>
      </Label>
      <Button fx:id="submitButton" layoutX="283.0" layoutY="285.0" mnemonicParsing="false" onAction="#switchScenes" styleClass="backButton" stylesheets="@Stylesheet.css" text="Back" textFill="WHITE" />
      <Button fx:id="viewButton" layoutX="348.0" layoutY="285.0" mnemonicParsing="false" onAction="#switchScenes" styleClass="confessionPostInfo" stylesheets="@Stylesheet.css" text="Go to confession page" />
      <Label layoutX="167.0" layoutY="240.0" text="Your post was submitted at :">
         <font>
            <Font name="PP Pangram Sans Light" size="14.0" />
         </font>
      </Label>
      <Label fx:id="st" layoutX="361.0" layoutY="240.0" onInputMethodTextChanged="#switchScenes" prefHeight="17.0" prefWidth="196.0" text="date" textFill="#2e35ff" />
      <Button layoutX="77.0" layoutY="395.0" maxHeight="48.0" maxWidth="81.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="81.0" styleClass="previous-button-icon" stylesheets="@Stylesheet.css" text="Button" />
      <Button layoutX="116.0" layoutY="395.0" maxHeight="48.0" maxWidth="81.0" mnemonicParsing="false" onAction="#stopMusic" prefHeight="48.0" prefWidth="81.0" styleClass="pause-button-icon" stylesheets="@Stylesheet.css" text="Button" />
      <Button layoutX="155.0" layoutY="395.0" maxHeight="48.0" maxWidth="81.0" mnemonicParsing="false" onAction="#continueMusic" prefHeight="48.0" prefWidth="81.0" styleClass="play-button-icon" stylesheets="@Stylesheet.css" text="Button" />
      <Button layoutX="190.0" layoutY="395.0" maxHeight="48.0" maxWidth="81.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="81.0" styleClass="next-button-icon" stylesheets="@Stylesheet.css" text="Button" />
   </children>
</AnchorPane>
